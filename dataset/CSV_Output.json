{
	"name": "CSV_Output",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_ADLS_Storage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"pYear": {
				"type": "string",
				"defaultValue": "2025"
			},
			"pMonth": {
				"type": "string",
				"defaultValue": "12"
			},
			"pDay": {
				"type": "string",
				"defaultValue": "14"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"container": {
					"value": "@concat('adf-output/', dataset().pYear, '/', dataset().pMonth,'/',dataset().pDay)",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}