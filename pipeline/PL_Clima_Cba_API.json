{
	"name": "PL_Clima_Cba_API",
	"properties": {
		"activities": [
			{
				"name": "CopyData_Clima_Cba",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.00:10:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_Open_Meteo_http",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_Blob_Storage_JSON",
						"type": "DatasetReference",
						"parameters": {
							"p_filename": {
								"value": "@concat(formatDateTime(addHours(utcnow(), -3), 'yyyy-MM-dd'), '_clima_cba.json')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"folder": {
			"name": "TrabajoIntegrador/01-Ingesta"
		},
		"annotations": []
	}
}